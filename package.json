{
	"name": "@maal/svelte-data",
	"version": "0.1.5",
	"description": "Package aiming to make it easier to manage data with advanced features in Svelte apps",
	"author": "Mathias Amandus Andvik LÃ¸ken",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"build:sandbox": "vite build",
		"preview": "vite preview",
		"test": "playwright install && playwright test",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"inspect": "pnpm check && pnpm lint",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./async": {
			"types": "./dist/async/index.d.ts",
			"svelte": "./dist/async/index.js"
		},
		"./async/AsyncDataComponent.svelte": {
			"types": "./dist/async/AsyncDataComponent.svelte.d.ts",
			"svelte": "./dist/async/AsyncDataComponent.svelte"
		},
		"./history": {
			"types": "./dist/history/index.d.ts",
			"svelte": "./dist/history/index.js"
		},
		"./http": {
			"types": "./dist/http/index.d.ts",
			"svelte": "./dist/http/index.js"
		},
		"./sync": {
			"types": "./dist/sync/index.d.ts",
			"svelte": "./dist/sync/index.js"
		},
		"./types": {
			"types": "./dist/types/index.d.ts",
			"svelte": "./dist/types/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.58.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.32.3",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.7",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"publint": "^0.1.11",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.1"
	},
	"type": "module"
}
